{
	"name": "WaveDrom",
	"version": "1.0.0, 2016-05-24",
	"author": ["[https://github.com/Martoni/mediawiki_wavedrom Some Guy]"],
	"url": "https://github.com/Martoni/mediawiki_wavedrom",
	"descriptionmsg": "wavedrom-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"MessagesDirs": {
		"WaveDrom": [
			"i18n"
		]
	},
	"Hooks": {
		"ParserFirstCallInit": "WaveDrom::onParserFirstCallInit"
	},
	"AutoloadClasses": {
		"WaveDrom": "WaveDrom_body.php"
	},
	"attributes": {
		"VisualEditor": {
			"PluginModules": [
				"ext.wavedrom.visualEditor"
			]
		}
	},
	"ResourceModules": {
		"ext.wavedrom.library.skin.default": {
			"scripts": "wavedrom/skins/default.js",
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.wavedrom.library": {
			"scripts": "wavedrom/wavedrom.js",
			"dependencies": [
				"ext.wavedrom.library.skin.default"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.wavedrom.postprocessing": {
			"scripts": [
				"wavedromframe.js"
			],
			"styles": [
				"wavedromframe.less"
			],
			"dependencies": [
				"ext.wavedrom.library"
			],
			"messages": [
				"visualeditor-mwwavedrom-tool"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.wavedrom.visualEditor": {
			"scripts": [
				"ve/ve.dm.MWWavedromNode.js",
				"ve/ve.ce.MWWavedromNode.js",
				"ve/ve.ui.MWWavedromDialogTool.js",
				"ve/ve.ui.MWWavedromDialog.js",
				"ve/ve.ui.MWWavedromContextItem.js"
			],
			"styles": [
				"ve/ve.ui.MWWavedromDialog.less"
			],
			"dependencies": [
				"ext.wavedrom.library",
				"jquery.throttle-debounce",
				"ext.visualEditor.mwcore",
				"oojs-ui.styles.icons-content",
				"oojs-ui.styles.icons-location"
			],
			"messages": [
				"visualeditor-mwwavedrom-tool",
				"visualeditor-mwwavedromdialog-align",
				"visualeditor-mwwavedromdialog-wavejson",
				"visualeditor-mwwavedromdialog-size",
				"visualeditor-mwwavedromdialog-title",
				"visualeditor-mwwavedromcontextitem-title",
				"visualeditor-mwwavedromcontextitem-nosupport",
				"visualeditor-mwwavedromdialog-position-button",
                "visualeditor-mwwavedromdialog-reset-map",
                "visualeditor-mwwavedrom-style-title",
                "visualeditor-mwwavedrom-style-title-help",
				"visualeditor-mwwavedrom-pulldownA",
				"visualeditor-mwwavedrom-pulldownB",
				"visualeditor-mwwavedrom-pulldownC"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "WaveDrom/modules"
	},
	"config": {
		"WaveDromURL": {
			"description": "Location of the WaveDrom WebApp",
			"public": true,
			"value": "https://wavedrom.com/"
		},
		"WaveDromDebug": {
			"description": "It does not minify the WaveDrom WebApp javascript file when it is true",
			"public": true,
			"value": false
		}
	},
	"manifest_version": 2
}
